<h1>Photos#new</h1>

<%= form_with(model: @photo, local: true ,url: confirm_photos_path) do |form| %>
    
  <div class="photo_content">
    <%= form.label :コメント %>
    <%= form.text_field :content %>
  </div>

  <div class="field">
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>

    <%= form.submit %>
<% end %>





<p>Find me in app/views/photos/new.html.erb</p>

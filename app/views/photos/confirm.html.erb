<h3>以下の内容で、送信する。</h3>

<%= form_with(model: @photo, local: true) do |form| %>
  <%= form.hidden_field :content %>
  <%= form.hidden_field :image_cache %>

  <p>コメント:<%= @photo.content %></p>
  <p>画像:

  <%= image_tag(@photo.image.url) if @photo.image && @photo.image.url %>
  <%= form.hidden_field :image_cache %>

  <p><%= form.submit  "投稿する" %></p>
  <% end %>